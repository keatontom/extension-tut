{
  "name": "extension-tut",
  "displayName": "extension-tut",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "extension-tut-sidebar",
          "title": "Sidebar",
          "icon": "media/queens.png"
        }
      ]
    },
    "views": {
      "extension-tut-sidebar": [
        {
          "type": "webview",
          "id": "extension-tut-sidebar-view",
          "name": "Sidebar View"
        }
      ]
    },
    "commands": [
      {
        "command": "extension-tut.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension-tut.askQuestion",
        "title": "Ask Question"
      },
      {
        "command": "extension-tut.touch",
        "title": "Create File"
      },
      {
        "command": "extension-tut.echo",
        "title": "Write to File"
      },
      {
        "command": "extension-tut.showWebview",
        "title": "Show Webview"
      }
    ],
    "languages": [
      {
          "id": "queens",
          "aliases": [
              "queens",
              "queens"
          ],
          "extensions": [
              ".queens"
          ],
          "configuration": "./queens_syntax_highliting/queens.configuration.json"
      }
  ],
  "grammars": [
      {
          "language": "queens",
          "scopeName": "source.queens",
          "path": "./queens_syntax_highliting/queens.tmLanguage"
      }
  ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
